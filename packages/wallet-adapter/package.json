{
  "name": "@kunalabs-io/sui-snap-wallet",
  "version": "0.1.1",
  "license": "Apache-2.0",
  "author": "Kuna Labs <build@kunalabs.io>",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "source": "./src/types.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    }
  },
  "files": [
    "dist/",
    "src/",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf tsconfig.tsbuildinfo ./dist",
    "build": "pnpm clean && pnpm tsc --build && pnpm tsup ./src/index.ts ./src/types.ts --format esm,cjs --sourcemap",
    "dev": "pnpm build --watch",
    "check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.2",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@metamask/detect-provider": "^2.0.0",
    "@metamask/providers": "^11.1.1",
    "@mysten/sui.js": "^0.41.0",
    "@mysten/wallet-adapter-base": "^0.9.0",
    "@mysten/wallet-standard": "^0.7.0",
    "superstruct": "^1.0.3"
  }
}
