{
  "name": "@kunalabs-io/sui-snap-wallet",
  "version": "0.4.3",
  "license": "Apache-2.0",
  "author": "Kuna Labs <build@kunalabs.io>",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "source": "./src/types.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    }
  },
  "files": [
    "dist/",
    "src/",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunalabs-io/sui-snap.git"
  },
  "scripts": {
    "clean": "rm -rf tsconfig.tsbuildinfo ./dist",
    "build": "pnpm clean && pnpm tsup ./src/index.ts ./src/types.ts --format esm,cjs --sourcemap --experimental-dts",
    "dev": "pnpm build --watch",
    "check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.42.2",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@metamask/detect-provider": "^2.0.0",
    "@metamask/providers": "^15.0.0",
    "@mysten/sui.js": "^0.50.1",
    "@mysten/wallet-adapter-base": "^0.9.0",
    "@mysten/wallet-standard": "^0.10.3",
    "superstruct": "^1.0.3"
  }
}
