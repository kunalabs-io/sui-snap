{
  "name": "@kunalabs-io/sui-snap-wallet-adapter",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "author": "Kuna Labs <build@kunalabs.io>",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "source": "./src/types.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    }
  },
  "scripts": {
    "clean": "rm -rf tsconfig.tsbuildinfo ./dist",
    "build": "pnpm clean && pnpm tsc --build && pnpm tsup ./src/index.ts ./src/types.ts --format esm,cjs --sourcemap",
    "dev": "pnpm build --watch",
    "check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@types/node": "^18.17.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "tsup": "^6.7.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@metamask/detect-provider": "^2.0.0",
    "@metamask/providers": "^11.1.0",
    "@mysten/sui.js": "^0.37.1",
    "@mysten/wallet-adapter-base": "^0.7.12",
    "@mysten/wallet-standard": "^0.5.12",
    "superstruct": "^1.0.3"
  }
}
