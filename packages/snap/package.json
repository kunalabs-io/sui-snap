{
  "name": "@kunalabs-io/sui-metamask-snap",
  "version": "0.1.0",
  "author": "Kuna Labs <build@kunalabs.io>",
  "main": "./dist/bundle.js",
  "license": "Apache-2.0",
  "files": [
    "dist/",
    "images/",
    "snap.manifest.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kunalabs-io/sui-snap"
  },
  "scripts": {
    "build": "mm-snap build",
    "build:clean": "pnpm clean && pnpm build",
    "build:website": "node ./scripts/build-website.js",
    "clean": "rimraf dist",
    "lint": "pnpm lint:eslint && pnpm lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "pnpm lint:eslint --fix && pnpm lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "serve": "mm-snap serve",
    "dev": "mm-snap watch"
  },
  "dependencies": {
    "@kunalabs-io/sui-snap-wallet-adapter": "workspace:*",
    "@metamask/key-tree": "^9.0.0",
    "@metamask/snaps-types": "^0.32.2",
    "@metamask/snaps-ui": "^0.32.2",
    "@mysten/sui.js": "^0.37.1",
    "@mysten/wallet-standard": "^0.5.12",
    "@noble/hashes": "^1.3.1",
    "buffer": "^6.0.3"
  },
  "devDependencies": {
    "@lavamoat/allow-scripts": "^2.3.1",
    "@metamask/auto-changelog": "^3.2.0",
    "@metamask/eslint-config": "^12.1.0",
    "@metamask/eslint-config-jest": "^12.1.0",
    "@metamask/eslint-config-nodejs": "^12.1.0",
    "@metamask/eslint-config-typescript": "^12.1.0",
    "@metamask/snaps-cli": "^0.32.2",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jsdoc": "^46.4.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-packagejson": "^2.4.5",
    "rimraf": "^5.0.1",
    "through2": "^4.0.2",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
